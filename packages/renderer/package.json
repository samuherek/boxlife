{
  "name": "@electra/electron",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron-starter.ts",
  "homepage": "./",
  "dependencies": {
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test",
    "eject": "react-scripts-ts eject",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "svgr": "npx @svgr/cli --config-file .svgrrc.js --ext tsx icons --out-dir src/components/icons",
    "electron": "npm run build:tsc && electron ./build/electron-starter.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@reach/router": "1.2.1",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "@types/reach__router": "1.2.3",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "electron": "4.0.2",
    "react-scripts-ts": "3.1.0",
    "typescript": "3.2.4"
  }
}
